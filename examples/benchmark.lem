function fibonacci (x) {
  if (x == 0) {
    return 0;
	}
	
	if (x == 1) {
		return 1;
	}

	// Printing a progress bar,
	if (x == 25) {
		print("â– ");
	}
	
	return fibonacci(x - 1) + fibonacci(x - 2);
};

