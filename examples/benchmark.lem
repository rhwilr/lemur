let fibonacci = function(x) {
  if (x == 0) {
    return 0;
	}
	
	if (x == 1) {
		return 1;
	}

	if (x == 25) {
		print(".");
	}
	
	fibonacci(x - 1) + fibonacci(x - 2);
};

println("Welcome to the Lemur benchmark.");
println("Running benchmark for Lemur engine: " + env("LEMUR_RUNTIME"));

println();
println("|--------------------- Progress --------------------------------------------------------|");

fibonacci(35);
println();
println();